{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "resolveJsonModule":true,
    "esModuleInterop": true,
    "outDir": "dist"

  },
  "include": ["vite.config.ts", "src", "src/**/*.ts", "src/**/*.tsx", "build", "build/**/*.json"],
}
